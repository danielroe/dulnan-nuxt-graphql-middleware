{
  "name": "nuxt-graphql-middleware",
  "version": "2.2.0",
  "description": "Module to perform GraphQL requests as a server middleware.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dulnan/nuxt-graphql-middleware.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jan Hug",
    "email": "me@dulnan.net",
    "url": "https://dulnan.net"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./module.cjs"
    },
    "./dist/*": "./dist/*"
  },
  "main": "./module.cjs",
  "types": "./types/index.d.ts",
  "files": [
    "types/*",
    "module.cjs",
    "dist/*.js",
    "dist/*.mjs",
    "dist/*.d.ts",
    "dist/types/*"
  ],
  "scripts": {
    "build": "unbuild && npm run generate-types",
    "dev": "unbuild --stub=true",
    "generate-types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "plain-ts": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/schema-ast": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@graphql-fragment-import/lib": "^1.2.0",
    "express": "^4.17.1",
    "graphql": "^16.0.1",
    "graphql-request": "^3.6.1"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.11.10",
    "consola": "^2.15.3",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "mkdirp": "^1.0.4",
    "mkdist": "latest",
    "nuxt": "^2.15.8",
    "prettier": "^2.5.0",
    "typescript": "^4.5.2",
    "unbuild": "^0.5.13",
    "vue": "^2.6.14"
  }
}
